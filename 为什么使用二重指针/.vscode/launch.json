  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) doublePointV1",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/doublePointV1",
      //"processId": "${command:pickProcess}",
      "cwd": "${workspaceRoot}",
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },

